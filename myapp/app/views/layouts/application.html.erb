<!DOCTYPE html>
<html>

<head>
  <title><%= t('common.main_title')%></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body id="body">
  <div id="container">
    <%= render :partial => '/parts/header' %>
