<h1>Listing stamps</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Issue date</th>
    <th>Status</th>
    <th>Description</th>
    <th>Image path</th>
    <th>Issue area</th>
    <th>Realm</th>
    <th>Series</th>
    <th>Issue price</th>
    <th>Remark1</th>
    <th>Remark2</th>
    <th>Remark3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stamps.each do |stamp| %>
  <tr>
    <td><%= stamp.name %></td>
    <td><%= stamp.issue_date %></td>
    <td><%= stamp.status %></td>
    <td><%= stamp.description %></td>
    <td><%= stamp.image_path %></td>
    <td><%= stamp.issue_area %></td>
    <td><%= stamp.realm %></td>
    <td><%= stamp.series %></td>
    <td><%= stamp.issue_price %></td>
    <td><%= stamp.remark1 %></td>
    <td><%= stamp.remark2 %></td>
    <td><%= stamp.remark3 %></td>
    <td><%= link_to 'Show', stamp %></td>
    <td><%= link_to 'Edit', edit_stamp_path(stamp) %></td>
    <td><%= link_to 'Destroy', stamp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Stamp', new_stamp_path %>
