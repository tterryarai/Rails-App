
<%= render :partial => '/parts/title', :locals => {:val => t('common.update')} %>

<div id="operations">
<%= render :partial => '/parts/commands', :locals => {:command => 'edit' } %>
</div>

<div class="form_layout">

<%= form_for(@virtual_run) do |f| %>

<%= render :partial => '/parts/form_errors', :locals => {:obj => @virtual_run} %>

<div>
<fieldset>
  <%= f.label :last, t('virtual_run.last') %><br>
  <%= f.text_field :last %><br>
  <%= f.label :name, t('virtual_run.name') %><br>
  <%= f.text_field :name %><br>
  <%= f.submit t('common.update'), {:class => "actions"} %>
</fieldset>
</div>

<% end %>

</div>

